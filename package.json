{
  "name": "top-users",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "build": "tsc -p tsconfig.json",
    "migrate:run": "knex --knexfile src/infra/knexfile.ts migrate:up",
    "migrate:rollback": "knex --knexfile src/infra/knexfile.ts migrate:down",
    "seed:run": "knex --knexfile src/infra/knexfile.ts seed:run",
    "test": "jest --config jest.config.js",
    "test:e2e": "jest --config jest-e2e.json",
    "test:cov": "jest --config jest-e2e.json --coverage",
    "test:unit": "jest --config jest.config.js",
    "test:unit:cov": "jest --config jest.config.js --coverage"
  },
  "dependencies": {
    "@automapper/classes": "^8.8.1",
    "@automapper/core": "^8.8.1",
    "@automapper/nestjs": "^8.8.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.3.0",
    "@nestjs/microservices": "^10.3.0",
    "@nestjs/testing": "^11.1.6",
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.4.5",
    "knex": "^3.1.0",
    "nestjs-knex": "^3.0.0",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "testcontainers": "^11.5.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.0"
  }
}
